<script src="./lp/download/js_1705/dhtmlxcombo.js"></script>
<script>
  var myCombo;

  function doOnLoad() {
    myCombo = dhtmlXComboFromSelect('numSelect');
  }

  function AddId() {
    var arr = new Array();
    $("#lp-movie-container > .lp-movie-box .kikaku-no").each(function(e) {
      arr.push($(this).text().replace('レク企画書No：', ''));
      arr.sort();
    });
    var j = 0;
    var len = arr.length;
    while (j < len) {
      var no_el = '<option value=‘+arr[j]+’>' + arr[j] + '</option>';
      var list_el = document.getElementById('numSelect');
      $(list_el).append(no_el);
      j++;
    }
  }


  //window.onload = doOnLoad;
  window.addEventListener('load', function() {
    AddId();
  });
  window.addEventListener('load', function() {
    doOnLoad();
  });



  function navigatePage(value) {

    $('.lp-movie-box').css('border', '0px none');
    $(document.getElementsByClassName('srh-err')).css('display', 'none');

    if (value == "‘+arr[j]+’") {
      var id = $('.dhxcombo_option_selected').text();
    } else {
      var id = value;
    }
    //var id = $('.dhxcombo_option_selected').text();
    var judgeflag = true;

    $('#lp-content .lp-movie-box').find('.kikaku-no').each(function(index, element) {
      // console.log($(element).text());
      if (element.textContent.lastIndexOf("レク企画書No：" + id) > -1 && id != "") {
        var border = $(element).parent().parent();
        console.log(border);
        $("html,body").animate({
          scrollTop: $(border).offset().top
        });
        $(border).css('border', 'solid 7px #fbff04');
        judgeflag = true;
        judgeflaglength = true;
        return false;
      } else {
        judgeflag = false;
      }
    })

    if (!(judgeflag)) {
      $(document.getElementsByClassName('srh-err')).css('display', 'block');
    }
  }

  function hankaku2Zenkaku(str) {
    return str.replace(/[Ａ-Ｚａ-ｚ０-９]/g, function(s) {
      return String.fromCharCode(s.charCodeAt(0) - 0xFEE0);
    });
  }
</script>

<style>
  .lpMovieTextNew>div {
    margin-bottom: 10px;
  }

