<script>
  $(function() {
    $('.open_button').each(function() {
      var iframe = $(this).next().find('iframe');
      var url = iframe.attr('data-src');
      var id = url.match(/[\/?=]([a-zA-Z0-9_-]{11})[&\?]?/)[1];
      $(this).click(function() {
        iframe.after('<p><iframe src="https://www.youtube.com/embed/' + id + '" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe></p>').remove();
      });
    });
  });
</script>


