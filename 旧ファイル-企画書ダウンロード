    <script>
      $(function() {
        //ページトップへのスクロール
        //('#pagetop').click(function () {
        //id名#pagetopがクリックされたら、以下の処理を実行
        //    $("html,body").animate({scrollTop:0},'200');
        //});
        //ページトップの出現
        $('#pagetop').hide();
        $(window).scroll(function() {
          if ($(window).scrollTop() > 500) {
            $('#pagetop').slideDown(600);
          } else {
            $('#pagetop').slideUp(600);
          }
        });

      });

      $(function() {
        // #で始まるアンカーをクリックした場合に処理
        $('a[href^=#]').click(function() {
          // スクロールの速度
          var speed = 1200; // ミリ秒
          // アンカーの値取得
          var href = $(this).attr("href");
          // 移動先を取得
          var target = $(href == "#" || href == "" ? 'html' : href);
          // 移動先を数値で取得
          var position = target.offset().top;
          // スムーススクロール
          $('body,html').animate({
            scrollTop: position
          }, speed, 'easeOutQuint');
          // $('body,html').animate({scrollTop:position}, speed, 'swing');
          return false;
        });
      });

      $(function() {
        // #で始まるアンカーをクリックした場合に処理
        $('a[href^=#]').click(function() {
          // スクロールの速度
          var speed = 1200; // ミリ秒
          // アンカーの値取得
          var href = $(this).attr("href");
          // 移動先を取得
          var target = $(href == "#" || href == "" ? 'html' : href);
          // 移動先を数値で取得
          var position = target.offset().top;
          // スムーススクロール
          $('body,html').animate({
            scrollTop: position
          }, speed, 'easeOutQuint');
          // $('body,html').animate({scrollTop:position}, speed, 'swing');
          return false;
        });
      });
    </script>

    <!-- ここから　伏下が追加 -->
    <!-- ↓Jqueruy読み込み。他で読み込むならこれは不要です。-->
    <!--<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>-->

    <script>
      //検索ボタンを押した時に実行される関数
      function searchClick(myNum) {

        //もしプルダウンに値があったら。
        if (myNum) {

          //console.log(myNum); //プルダウンの中身確認

          //すでにchangedクラスが付与されていた場合は削除する
          $(".dlbox_new").removeClass("changed");

          //プルダウンと同じIDを探してその一つ前のdiv要素に changedクラスを付与する
          $('#' + myNum).prev("div").addClass("changed");

          //スクロール移動する
          navigatePage(document.form.recno.value);
        }
      }
    </script>


    <style type="text/css">
      /*スタイルシートで枠線をつける。*/
      .changed {
        background: #fffe6e;
      }
    </style>

    <!-- ここまで　伏下が追加 -->


    <!-- トップへ戻る -->
